pipeline {
    agent none
    stages {
        stage('Example Build') {
            agent { label 'any', docker 'maven:3-alpine' }
            steps {
                echo 'Hello, Maven'
                sh 'mvn --version || echo no'
            }
        }
        stage('Example Test') {
            //agent { docker 'openjdk:8-jre' }
            steps {
                echo 'Hello, JDK'
                sh 'java -version || echo no'
            }
        }
    }
}
