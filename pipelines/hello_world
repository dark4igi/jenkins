pipeline {
  agent any
  parameters {
        string(name: 'PERSON', defaultValue: 'Mr Jenkins', description: 'Who should I say hello to?')
  stages {
    stage ('Build') {
      steps {
        echo 'Hello World using Jenkins pipelines from git'
        echo "Hello ${params.PERSON}"
        sh 'ls -la'
        
      }
    }
  }
  post {
    always { cleanWs() }
  }
}
